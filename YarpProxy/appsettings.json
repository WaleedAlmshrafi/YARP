{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "YARP": {
    "Routes": {
      "TestApi": {
        "ClusterId": "apiTest",
        "Match": {
          "Path": "/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "apiTest": {
        "Destinations": {
          "TestApi": {
            "Address": "http://localhost:5114"
          }
        }
      }
    }
  }
}

